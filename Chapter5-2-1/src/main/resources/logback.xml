<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<property name="console.fmt"
			  value=" %date [%thread] %-5level %logger{80} [%line] - %msg %X{thirdUserId} %X{apply_id} %n " />
	<property name="LOG_PATH" value="/www/webcode/apollo-adminservice" />
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder charset="UTF-8">
			<pattern>  %date [%thread] %-5level %logger{80} [%line] %marker - %msg %n </pattern>
		</encoder>
	</appender>

	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>
